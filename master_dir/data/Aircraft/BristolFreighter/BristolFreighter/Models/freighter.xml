<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <model>
    <path>Aircraft/BristolFreighter/Models/freighter.ac</path>
  </model>

  <model>
    <name>Instruments</name>
    <path>Aircraft/BristolFreighter/Models/instruments.xml</path>
  </model>

  <model>
    <path>Aircraft/BristolFreighter/Models/Effects/cranking.xml</path>
  </model>

  <!-- *** Rudder -->
  <animation>
    <type>rotate</type>
    <object-name>rudder</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor>-28</factor>
    <axis>
      <x1-m>8.753</x1-m>   <!-- back -->
      <y1-m>0.0  </y1-m>   <!-- left/right -->
      <z1-m>5.734</z1-m>   <!-- Up -->
      <x2-m>8.716</x2-m>
      <y2-m>0.0  </y2-m>
      <z2-m>7.826</z2-m>
    </axis>
  </animation>
  
  
    <!-- *** Tail wheel -->

  <animation>
    <type>rotate</type>
    <object-name>tailWheel</object-name>
    <property>gear/gear[2]/caster-angle-deg</property>
    <factor>1</factor>
    <axis>
      <x1-m>8.900</x1-m>
      <y1-m>0.0  </y1-m>
      <z1-m>1.915</z1-m>
      <x2-m>8.845</x2-m>
      <y2-m>0.0  </y2-m>
      <z2-m>2.399</z2-m>
    </axis>
  </animation>


  <!-- *** Elevators -->
  <animation>
    <type>rotate</type>
    <object-name>elevators</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <interpolation>
      <entry>
        <ind> 1.0</ind>
        <dep> 27.0</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>-1.0</ind>
        <dep>-17.0</dep>
      </entry>
    </interpolation>    
    <center>
      <x-m> 9.140 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 3.154 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- *** Ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronL</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind> 1.0</ind>
        <dep> 19.0</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>-1.0</ind>
        <dep>-15.0</dep>
      </entry>
    </interpolation>    
    <axis>
      <x1-m>  -0.832 </x1-m>
      <y1-m> -13.860 </y1-m>
      <z1-m>   4.224 </z1-m>
      <x2-m>  -0.832 </x2-m>
      <y2-m>  -8.457 </y2-m>
      <z2-m>   4.019 </z2-m>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>aileronR</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <interpolation>
      <entry>
        <ind> 1.0</ind>
        <dep> 19.0</dep>
      </entry>
      <entry>
        <ind>0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>-1.0</ind>
        <dep>-15.0</dep>
      </entry>
    </interpolation>    
    <axis>
      <x1-m> -0.832 </x1-m>
      <y1-m>  8.457 </y1-m>
      <z1-m>  4.019 </z1-m>
      <x2-m> -0.832 </x2-m>
      <y2-m> 13.860 </y2-m>
      <z2-m>  4.224 </z2-m>
    </axis>
  </animation>

  <!-- *** Flaps -->
  <animation>
    <type>rotate</type>
    <object-name>flapOuterL</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60</factor>
    <axis>
      <x1-m> -1.100 </x1-m>
      <y1-m> -8.337 </y1-m>
      <z1-m>  3.815 </z1-m>
      <x2-m> -1.100 </x2-m>
      <y2-m> -4.704 </y2-m>
      <z2-m>  3.652 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flapInnerL</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60</factor>
    <axis>
      <x1-m> -1.108 </x1-m>
      <y1-m> -4.566 </y1-m>
      <z1-m>  3.650 </z1-m>
      <x2-m> -1.108 </x2-m>
      <y2-m> -1.393 </y2-m>
      <z2-m>  3.650 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flapInnerR</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60</factor>
    <axis>
      <x1-m> -1.108 </x1-m>
      <y1-m>  1.393 </y1-m>
      <z1-m>  3.650 </z1-m>
      <x2-m> -1.108 </x2-m>
      <y2-m>  4.566 </y2-m>
      <z2-m>  3.650 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>flapOuterR</object-name>
    <property>surface-positions/flap-pos-norm</property>
    <factor>60</factor>
    <axis>
      <x1-m> -1.100 </x1-m>
      <y1-m>  4.704 </y1-m>
      <z1-m>  3.652 </z1-m>
      <x2-m> -1.100 </x2-m>
      <y2-m>  8.337 </y2-m>
      <z2-m>  3.815 </z2-m>
    </axis>
  </animation>
  
  <!-- *** Nose doors -->
  <animation>
    <type>rotate</type>
    <object-name>noseDoorL</object-name>
    <object-name>noseDoorL_interior</object-name>
    <object-name>noseDoorL_framework</object-name>
    <object-name>noseDoorL_brace</object-name>
    <object-name>noseDoorL_hinges</object-name>
    <object-name>noseDoorL_transparent</object-name>
    <property>instrumentation/doors/noseDoors/position-norm</property>
    <factor>-90</factor>
    <axis>
      <x1-m> -8.010 </x1-m>
      <y1-m> -1.355 </y1-m>
      <z1-m>  3.250 </z1-m>
      <x2-m> -8.010 </x2-m>
      <y2-m> -1.355 </y2-m>
      <z2-m>  1.430 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>noseDoorR</object-name>
    <object-name>noseDoorR_interior</object-name>
    <object-name>noseDoorR_framework</object-name>
    <object-name>noseDoorR_brace</object-name>
    <object-name>noseDoorR_hinges</object-name>
    <object-name>noseDoorR_transparent</object-name>
    <property>instrumentation/doors/noseDoors/position-norm</property>
    <factor>90</factor>
    <axis>
      <x1-m> -8.010 </x1-m>
      <y1-m>  1.355 </y1-m>
      <z1-m>  3.250 </z1-m>
      <x2-m> -8.010 </x2-m>
      <y2-m>  1.355 </y2-m>
      <z2-m>  1.430 </z2-m>
    </axis>
  </animation>  
  
  
  <!-- *** Props -->

  <!-- Left prop -->
  <animation>
    <type>spin</type>
    <object-name>propL</object-name>
    <object-name>propL_spinner</object-name>
    <object-name>propL_fan</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[0]/rpm</property>
          <value>1.0</value>
        </greater-than>
        <less-than>
          <property>engines/engine[0]/rpm</property>
          <value>1200.0</value>
        </less-than>
      </and> 
    </condition> 
	<property>engines/engine[0]/rpm</property>
    <factor>0.444</factor>
    <axis>
      <x> 1.0 </x>
      <y> 0.0 </y>
      <z> 0.0 </z>
    </axis>
    <center>
      <x-m> 7.324</x-m>
      <y-m>-3.907</y-m>
      <z-m> 3.413</z-m>
    </center>  
  </animation>

  <animation>
    <type>select</type>
    <object-name>propL</object-name>
    <object-name>propL_fan</object-name>
    <object-name>propL_spinner</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1200</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>propBlur4L</object-name>
    <object-name>propBlurL_spinner</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1200</value>
      </greater-than>
    </condition>
  </animation>

  <!-- Right prop -->
  <animation>
    <type>spin</type>
    <object-name>propR</object-name>
    <object-name>propR_spinner</object-name>
    <object-name>propR_fan</object-name>
    <condition>
      <and>
        <greater-than>
          <property>engines/engine[1]/rpm</property>
          <value>1.0</value>
        </greater-than>
        <less-than>
          <property>engines/engine[1]/rpm</property>
          <value>1200.0</value>
        </less-than>
      </and> 
    </condition> 
	<property>engines/engine[1]/rpm</property>
    <factor>0.444</factor>
    <axis>
      <x> 1.0 </x>
      <y> 0.0 </y>
      <z> 0.0 </z>
    </axis>
    <center>
      <x-m> 7.324</x-m>
      <y-m> 3.907</y-m>
      <z-m> 3.413</z-m>
    </center>  
  </animation>

  <animation>
    <type>select</type>
    <object-name>propR</object-name>
    <object-name>propR_fan</object-name>
    <object-name>propR_spinner</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
        <value>1200</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>propBlur4R</object-name>
    <object-name>propBlurR_spinner</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>1200</value>
      </greater-than>
    </condition>
  </animation>
  
</PropertyList>

